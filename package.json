    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.1",
    "express-validator": "^6.14.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
